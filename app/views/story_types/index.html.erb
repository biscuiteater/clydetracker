<h1>Listing story_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @story_types.each do |story_type| %>
  <tr>
    <td><%=h story_type.name %></td>
    <td><%=h story_type.description %></td>
    <td><%= link_to 'Show', story_type %></td>
    <td><%= link_to 'Edit', edit_story_type_path(story_type) %></td>
    <td><%= link_to 'Destroy', story_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New story_type', new_story_type_path %>